{
  "format_version": "1.2",
  "terraform_version": "1.5.0",
  "resource_changes": [
    {
      "address": "aws_security_group.web",
      "type": "aws_security_group",
      "change": {
        "actions": ["create"],
        "after": {
          "name": "web-sg",
          "ingress": [
            {
              "from_port": 443,
              "to_port": 443,
              "protocol": "tcp",
              "cidr_blocks": ["0.0.0.0/0"]
            }
          ]
        }
      }
    },
    {
      "address": "aws_security_group.backend",
      "type": "aws_security_group",
      "change": {
        "actions": ["create"],
        "after": {
          "name": "backend-sg",
          "ingress": [
            {
              "from_port": 5432,
              "to_port": 5432,
              "protocol": "tcp",
              "cidr_blocks": ["10.0.0.0/8"]
            }
          ]
        }
      }
    },
    {
      "address": "aws_lb.web",
      "type": "aws_lb",
      "change": {
        "actions": ["create"],
        "after": {
          "name": "web-alb",
          "internal": false,
          "security_groups": ["aws_security_group.web"]
        }
      }
    },
    {
      "address": "aws_instance.web",
      "type": "aws_instance",
      "change": {
        "actions": ["create"],
        "after": {
          "instance_type": "t3.micro",
          "associate_public_ip_address": true,
          "vpc_security_group_ids": ["aws_security_group.web"]
        }
      }
    },
    {
      "address": "aws_instance.backend",
      "type": "aws_instance",
      "change": {
        "actions": ["create"],
        "after": {
          "instance_type": "t3.small",
          "associate_public_ip_address": false,
          "vpc_security_group_ids": ["aws_security_group.backend"]
        }
      }
    },
    {
      "address": "aws_autoscaling_group.web",
      "type": "aws_autoscaling_group",
      "change": {
        "actions": ["create"],
        "after": {
          "name": "web-asg",
          "min_size": 1,
          "max_size": 3
        }
      }
    },
    {
      "address": "aws_db_instance.prod",
      "type": "aws_db_instance",
      "change": {
        "actions": ["create"],
        "after": {
          "identifier": "prod-db",
          "engine": "postgres",
          "publicly_accessible": false
        }
      }
    },
    {
      "address": "aws_iam_role.app",
      "type": "aws_iam_role",
      "change": {
        "actions": ["create"],
        "after": {
          "name": "app-role",
          "assume_role_policy": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Principal\":{\"Service\":\"ec2.amazonaws.com\"},\"Action\":\"sts:AssumeRole\"}]}"
        }
      }
    },
    {
      "address": "aws_iam_policy.wide",
      "type": "aws_iam_policy",
      "change": {
        "actions": ["create"],
        "after": {
          "name": "wide-policy",
          "policy": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Action\":\"*\",\"Resource\":\"*\"}]}"
        }
      }
    },
    {
      "address": "aws_iam_role_policy.inline",
      "type": "aws_iam_role_policy",
      "change": {
        "actions": ["create"],
        "after": {
          "name": "inline-policy",
          "role": "aws_iam_role.app",
          "policy": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Action\":[\"rds:*\",\"iam:PassRole\"],\"Resource\":\"*\"}]}"
        }
      }
    },
    {
      "address": "aws_iam_role_policy_attachment.wide",
      "type": "aws_iam_role_policy_attachment",
      "change": {
        "actions": ["create"],
        "after": {
          "role": "aws_iam_role.app",
          "policy_arn": "aws_iam_policy.wide"
        }
      }
    },
    {
      "address": "aws_iam_policy_attachment.extra",
      "type": "aws_iam_policy_attachment",
      "change": {
        "actions": ["create"],
        "after": {
          "name": "extra-attachment",
          "roles": ["aws_iam_role.app"],
          "policy_arn": "aws_iam_policy.wide"
        }
      }
    }
  ]
}
